"            text = recognizer.recognize_google(audio, language='id-ID')\n","            return text\n","        except sr.UnknownValueError:\n","            return \"Google Speech Recognition could not understand the audio\"\n","        except sr.RequestError as e:\n","            return f\"Could not request results from Google Speech Recognition service; {e}\"\n","\n","if __name__ == \"__main__\":\n","    # Loop untuk mentranskripsi setiap segmen\n","    for i in range(8):  # Total segmen yang sudah dipotong\n","        segment_file = f\"segment_{i}.wav\"\n","        print(f\"Mentranskripsi {segment_file}...\")\n","        transcription = transcribe_audio(segment_file)\n","        print(f\"Hasil transkripsi dari {segment_file}:\\n{transcription}\\n\")"]},{"cell_type":"code","execution_count":10,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"elapsed":147967,"status":"ok","timestamp":1734884810407,"user":{"displayName":"10221023 Ani Nuryani","userId":"16133244036368531487"},"user_tz":-420},"id":"3WJCMSn5IYRD","outputId":"31b66eec-8302-4c1a-8731-accce9411f2e"},"outputs":[{"output_type":"stream","name":"stdout","text":["Menggabungkan transkripsi dari semua segmen...\n","Mentranskripsi segment_0.wav...\n","Mentranskripsi segment_1.wav...\n","Mentranskripsi segment_2.wav...\n","Mentranskripsi segment_3.wav...\n","Mentranskripsi segment_4.wav...\n","Mentranskripsi segment_5.wav...\n","Mentranskripsi segment_6.wav...\n","Mentranskripsi segment_7.wav...\n","\n","Transkripsi Gabungan:\n","\n","pemuda-pemudi muslim pada saat hari natal pada saat tahun baru pada hari-hari besar minoritas berdiri di depan tempat ibadahnya agama lain menjaga Supaya umat agama lain beribadah dengan tenang di tempatnya mereka-mereka muslim-muslim pemuda Muslim yang menjaga di luar ini leadership nya Gus Dur tapi mereka bertanya-tanya saya sampaikan Sabar sedikit saya baru menjabat 2 bulan 2 bulan ya kan anda sudah Hitung berapa koruptor yang sudah ditangkap Bismillah